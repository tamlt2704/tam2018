import pandas as pd
import json

busgeo_df = pd.read_csv('static/BusGeo.csv', index_col=[0])

for k, g in busgeo_df.groupby(['Code', 'Direction']):
    js = g.sort_values('GeoSequence')[['Lat', 'Lng']]
    js = js.to_dict()
    filename = 'static/{}_{}.json'.format(k[0], k[1])
    with open(filename, 'w') as f:
        json.dump(js, f)
